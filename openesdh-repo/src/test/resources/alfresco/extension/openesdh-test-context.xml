<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
    <bean id="TestCaseHelper" class="dk.openesdh.repo.helper.CaseHelper">
        <property name="nodeService">
            <ref bean="NodeService"/>
        </property>

        <property name="searchService">
            <ref bean="SearchService"/>
        </property>

        <property name="authorityService">
            <ref bean="AuthorityService"/>
        </property>

        <property name="permissionService">
            <ref bean="PermissionService"/>
        </property>

        <property name="repositoryHelper">
            <ref bean="repositoryHelper"/>
        </property>

        <property name="nodeLocatorService">
            <ref bean="nodeLocatorService"/>
        </property>

        <property name="retryingTransactionHelper">
            <ref bean="retryingTransactionHelper"/>
        </property>

        <property name="personService">
            <ref bean="personService"/>
        </property>

        <property name="authenticationService">
            <ref bean="authenticationService"/>
        </property>

    </bean>
	
	<bean id="CaseDocumentTestHelper" class="dk.openesdh.repo.helper.CaseDocumentTestHelper">
		<property name="nodeService">
            <ref bean="NodeService"/>
        </property>
        <property name="repositoryHelper">
            <ref bean="repositoryHelper"/>
        </property>
        <property name="caseHelper">
            <ref bean="TestCaseHelper"/>
        </property>
        <property name="caseService">
            <ref bean="CaseService"/>
        </property>
        <property name="documentService">
            <ref bean="DocumentService"/>
        </property>
	</bean>

</beans>
