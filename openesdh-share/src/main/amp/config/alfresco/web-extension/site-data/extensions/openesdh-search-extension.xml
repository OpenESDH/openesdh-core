<extension>
    <modules>
        <module>
            <id>OpenESDH Search</id>
            <version>1.0</version>
            <auto-deploy>true</auto-deploy>
            <evaluator type="dk.openesdh.evaluator.uriprefix">
                <params>
                    <uriPrefix>/share/page/hdp/ws/search</uriPrefix>
                    <apply>true</apply>
                </params>
            </evaluator>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.share.header</targetPackageRoot>
                    <sourcePackageRoot>dk.openesdh.search-header</sourcePackageRoot>
                </customization>
            </customizations>
        </module>
    </modules>
</extension>