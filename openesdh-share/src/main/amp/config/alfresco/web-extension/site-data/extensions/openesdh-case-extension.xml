<extension>
    <modules>
        <module>
            <id>OpenESDH Case Extensions</id>
            <version>1.1</version>
            <auto-deploy>true</auto-deploy>
            <evaluator type="openesdh.caseModuleEvaluator">
                <params>
                    <caseType>.*</caseType>
                    <!--<caseStatus>.*</caseStatus>-->
                    <apply>true</apply>
                </params>
            </evaluator>

            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.share.header</targetPackageRoot>
                    <sourcePackageRoot>dk.openesdh.case.pages.navigation.header</sourcePackageRoot>
                </customization>
                <customization>
                    <targetPackageRoot>org.alfresco.components.create-content</targetPackageRoot>
                    <sourcePackageRoot>dk.openesdh.case.components.create-content</sourcePackageRoot>
                </customization>
            </customizations>
        </module>

